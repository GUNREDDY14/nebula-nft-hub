# ===========================================
# NFT MARKETPLACE ENVIRONMENT CONFIGURATION
# ===========================================
# Copy this file to .env and fill in your values

# ----- BLOCKCHAIN CONFIGURATION -----
# Contract address (will be auto-generated after deployment)
VITE_CONTRACT_ADDRESS=

# Network to use: "localhost" or "sepolia" or "mumbai"
VITE_NETWORK=localhost

# ----- PINATA IPFS CONFIGURATION -----
# Get your API keys from: https://app.pinata.cloud/keys
VITE_PINATA_API_KEY=
VITE_PINATA_SECRET_API_KEY=
VITE_PINATA_JWT=

# ----- TESTNET RPC URLs (for deployment) -----
# Get free RPC URLs from Alchemy or Infura
# Sepolia: https://dashboard.alchemy.com
SEPOLIA_RPC_URL=

# Mumbai (Polygon testnet)
MUMBAI_RPC_URL=

# ----- WALLET CONFIGURATION (for deployment only) -----
# Your wallet private key (NEVER share this or commit to git!)
# Export from MetaMask: Settings > Security > Export Private Key
PRIVATE_KEY=

# ----- OPTIONAL: ETHERSCAN VERIFICATION -----
# For verifying contracts on Etherscan
ETHERSCAN_API_KEY=
